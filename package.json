{
  "name": "@pinyin/dom",
  "version": "0.0.7",
  "description": "DOM Utilites",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepublishOnly": "rm -rf dist && npx tsc",
    "test": "npx tsc && npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinyin/dom.git"
  },
  "keywords": [
    "DOM"
  ],
  "author": "Bo Bao <bob432091@gmail.com> (https://github.com/pinyin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pinyin/dom/issues"
  },
  "homepage": "https://github.com/pinyin/dom#readme",
  "dependencies": {
    "@pinyin/maybe": "^0.0.9",
    "@pinyin/types": "^0.0.45",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "jest": "^23.4.0",
    "ts-jest": "^22.0.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "verbose": true,
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": true
      }
    }
